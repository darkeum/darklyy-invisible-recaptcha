{
    "name": "darkeum/darklyy-invisible-recaptcha",
    "description": "Добавляет Invisible reCAPTCHA в Darklyy.",
    "keywords": [
        "darkeum",
        "darklyy",
        "recaptcha",
        "invisible-recaptcha",
        "darklyy-invisible-recaptcha"
    ],
    "homepage": "https://github.com/darkeum/darklyy-invisible-recaptcha",
    "license": "MIT",
    "authors": [{
        "name": "Komarov Ivan",
        "email": "ki@darkeum.ru",
        "homepage": "https://darkeum.ru",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6.4 || ^7.0 || ^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/view": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "darkeum/darklyy-package-tools": "^1.0.0"
    },
    "require-dev": {
        "php": "^5.6.4 || ^7.0 || ^8.0",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/view": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "darkeum/darklyy-package-tools": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Darkeum\\InvisibleReCaptcha\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "darklyy": {
            "providers": [
                "Darkeum\\JsonAttributes\\InvisibleReCaptchaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}